    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gaurang.pdfspeaker.about_us">
    <TextView
        android:id="@+id/about_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:text="about us"
        android:textAllCaps="true"
        android:textSize="25dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/about_us"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:text="Gaurang Dobariya\ndobariyagaurang@gmail.com" />

    <TextView
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:text="Naimish Desai\n15it020@charusat.edu.in"
        android:layout_below="@+id/one"
        android:layout_alignStart="@+id/one" />
